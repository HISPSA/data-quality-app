  "private": true,
  "dependencies": {
    "d2": "^28.3.0",
    "d2-manifest": "^1.0.0",
    "d2-ui": "^28.0.8",
    "d2-utilizr": "^0.2.15",
    "loglevel": "^1.6.0",
    "material-ui": "^0.20.0",
